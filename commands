GCP create managed instance group : 

    gcloud beta compute --project=mynewhomeproj instance-groups managed create my-managed-instance-group --base-instance-name=my-managed-instance-group --template=worker-instance-template-1 --size=1 --zones=us-central1-b,us-central1-c,us-central1-f,us-central1-a --instance-redistribution-type=PROACTIVE

    gcloud beta compute --project "mynewhomeproj" instance-groups managed set-autoscaling "my-managed-instance-group" --region "us-central1" --cool-down-period "30" --max-num-replicas "5" --min-num-replicas "1" --target-cpu-utilization "0.3" --mode "on"
